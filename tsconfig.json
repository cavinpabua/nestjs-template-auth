{
  "compilerOptions": {
    "strict": true,
    "alwaysStrict": true,
    "noImplicitAny": false,
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2017",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "strictBindCallApply": false,
    "forceConsistentCasingInFileNames": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "@": [
        "src"
      ],
      "@/*": [
        "src/*"
      ],
      "filter2dbquery": [
        "libs/filter2dbquery/src"
      ],
      "filter2dbquery/*": [
        "libs/filter2dbquery/src/*"
      ]
    }
  },
  "include": ["src","libs", "test"],
  "exclude": ["**/node_modules", "**/.*/"]
}
